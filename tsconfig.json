{
  "compilerOptions": {
    "allowJs": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": ["dom", "es2017"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "esnext"
  },
  "exclude": ["node_modules"],
  "include": ["**/*.ts", "**/*.tsx", "pages/me/index.test.js"],
  "files": [
    "twin.d.ts",
    "./components/Layout/Logout.tsx",
    "./components/Layout/Logo.tsx",
    "./components/Layout/MotionLink.tsx",
    "./components/Layout/WrapperRightNavBar.tsx",
    "./components/Layout/WrapperLeftNavBar.tsx"
  ]
}
